#include <stdio.h>
#include <stdlib.h>
/**
* main - return the sum of two ints
* @argc: the no. of ints
* @argv: the two ints
* Return: the number of ints
*/
int main(int argc, char *argv[])
{
int result = 0;
int i;
for (i = 1; i < argc; i++)
{
if (argv[i][0] >= 'a' && argv[i][0] <= 'z')
{
printf("Error\n"i);
return (1);
}
}
for (i = 1; i < argc; i++)
{
result += atoi(argv[i]);
}
printf("%d\n", result);
return (0);
}
